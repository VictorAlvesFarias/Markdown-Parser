{
  "name": "markdown-parser-lib",
  "version": "1.0.0",
  "description": "A powerful and modular README markdown parser library",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./parser": "./dist/parser/index.js",
    "./builders": "./dist/builders/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.js --bundle --outfile=dist/index.js --format=esm",
    "dev": "esbuild src/index.js --bundle --outfile=dist/index.js --format=esm --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "markdown",
    "markdown",
    "parser",
    "library",
    "javascript",
    "es6"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.20.2",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/markdown-parser-lib.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/markdown-parser-lib/issues"
  },
  "homepage": "https://github.com/yourusername/markdown-parser-lib#markdown"
}
